<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
		 http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	version="3.1">
	<display-name>Spring MVC Web Application</display-name>

	<filter-mapping>
		<filter-name>jdbcFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>

	<filter-mapping>
		<filter-name>cookieFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>

	<welcome-file-list>

		<welcome-file>home</welcome-file>

		<welcome-file>index.html</welcome-file>

	</welcome-file-list>
	<servlet>
		<servlet-name>RegController</servlet-name>
		<servlet-class>fpt.finish.severlet.RegController</servlet-class>
		<init-param>
			<param-name>encoding</param-name>
			<param-value>UTF-8</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>RegController</servlet-name>
		<url-pattern>/reg</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>DangKyPmController</servlet-name>
		<servlet-class>fpt.finish.severlet.DangKyPmController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DangKyPmController</servlet-name>
		<url-pattern>/dangkypm</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>DangKyFormController</servlet-name>
		<servlet-class>fpt.finish.severlet.DangKyFormController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DangKyFormController</servlet-name>
		<url-pattern>/dangkypmform</url-pattern>
	</servlet-mapping>

</web-app>